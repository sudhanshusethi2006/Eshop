<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>sudhanshu's Eshop</title>
   

</head>
<body>


    <header class="conta">


        <nav class="navbar navbar-dark bg-dark navbar-expand-md">
            <h1 class="navbar-brand">Welcome to Sudhanshu's Laptop Eshop </h1>
            <button class="navbar-toggler">
                <span class="navbar-toggler-icon" data-toggle="collapse" data-target="#myMenu">

                </span>
            </button>

            <div id="myMenu" class="navbar-collapse collapse">
                <ul class="navbar-nav">
                    <li class="nav-item"><a class="nav-link" asp-controller="App" asp-action="Index">Home</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="App" asp-action="Contact">Contact</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="App" asp-action="About">About Us</a></li>

                </ul>
            </div>
        </nav>

    </header>
    <selection class="container">
        <h2>@ViewBag.Title</h2>
        @RenderBody()
    </selection>
    <footer>

        <div class="text-center">
            Copyright 2020 Sudhanshu's eshop

        </div>
    </footer>
    <link rel="stylesheet" href="~/css/site.css">
    <link href="~/node_modules/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/node_modules/jquery/dist/jquery.js"></script>
    <script src="~/node_modules/bootstrap/dist/js/bootstrap.bundle.js"></script>
    @*<script src="~/js/Index.js"></script>*@
    @RenderSection("scripts", false)
</body>
</html>